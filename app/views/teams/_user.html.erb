<li class="roster-list">
  <% if user == current_user %>
  <div class="roster-name"><%= link_to current_user.known_by, user_path %></div>
  <% else %>
  <div class="team-name"><%= user.known_by %></div>
  <% end %>
  <div class="team-results">
    <% @team_data[user.id].each do |k, v| %>
    <span class=""><%= sprintf('%d', v) %></span>
    <% end %>
  </div>
</li>
